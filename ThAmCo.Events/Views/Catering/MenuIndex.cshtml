@model IEnumerable<MenuVM>

@{
    ViewData["Title"] = "Index";
}

<h1>Menu List</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MenuName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MenuName)
                </td>

            </tr>
        }
    </tbody>
</table>

